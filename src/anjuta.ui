<!--*- xml -*-->
<ui>
	<menubar name="MenuMain">
		<menu name="MenuFile" action="ActionMenuFile">
			<placeholder name="PlaceholderFileMenus"/>
			<placeholder name="PlaceholderPrintMenus"/>
			<placeholder name="PlaceholderRecentMenus"/>
			<menuitem name="Quit" action="ActionExit" />
		</menu>
		<menu name="MenuEdit" action="ActionMenuEdit">
			<placeholder name="PlaceholderEditMenus"/>
			<placeholder name="PlaceholderSelectMenus"/>
			<placeholder name="PlaceholderInsertMenus"/>
			<placeholder name="PlaceholderSearchMenus"/>
		</menu>
		<menu name="MenuView" action="ActionMenuView">
			<menuitem name="Fullscreen" action="ActionViewFullscreen"/>
			<menuitem name="Toolbars" action="ActionViewToolbars"/>
		</menu>
		<placeholder name="PlaceholderGotoMenus"/>
		<placeholder name="PlaceHolderProjectMenus"/>
		<placeholder name="PlaceHolderFormatMenus"/>
		<placeholder name="PlaceHolderBuildMenus"/>
		<placeholder name="PlaceHolderDebugMenus"/>
		<placeholder name="PlaceHolderVersionControlMenus"/>
		<placeholder name="PlaceHolderBookmarkMenus"/>
		<menu name="MenuSettings" action="ActionMenuSettings" >
			<menuitem name="Preferences" action="ActionSettingsPreferences" />
			<menuitem name="Defaults" action="ActionSettingsDefaults" />
			<menuitem name="Shortcuts" action="ActionSettingsShortcuts" />
			<menuitem name="Layout Manager" action="ActionSettingsLayout" />
			<menuitem name="Plugins Manager" action="ActionSettingsPlugins" />
		</menu>
		<placeholder name="PlaceHolderHelpMenus" >
			<menu name="MenuHelp" action="ActionMenuHelp" >
				<placeholder name="PlaceHolderHelpDevel" >
						<separator name="separator1" />
						<menuitem name="Man" action="ActionHelpMan" />
						<menuitem name="Info" action="ActionHelpInfo" />
				</placeholder>
				<placeholder name="PlaceHolderHelpWeb" >
					<separator name="separator1" />
					<menuitem name="AnjutaHome" action="ActionHelpAnjutaHome" />
					<separator name="separator2" />
					<menuitem name="BugReport" action="ActionHelpBugReport" />
					<menuitem name="FetureRequest" action="ActionHelpFeatureRequest" />
					<menuitem name="SubmitPatch" action="ActionHelpSubmitPatch" />
					<menuitem name="Faq" action="ActionHelpFaq" />
				</placeholder>
				<placeholder name="PlaceHolderHelpAbout" >
					<separator name="separator1" />
					<menuitem name="AboutAnjuta" action="ActionAboutAnjuta" />
				</placeholder>
			</menu>
		</placeholder>
		<placeholder name="TestPlaceholder" />
	</menubar>
	<toolbar name="ToolbarMain"></toolbar>
	<toolbar name="ToolbarBrowser"></toolbar>
</ui>
