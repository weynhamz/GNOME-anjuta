					<menuitem name="Props" verb="ActionFormatHighlightStyleProps" />
					<menuitem name="Conf" verb="ActionFormatHighlightStyleConf" />
					<menuitem name="Ada" verb="ActionFormatHighlightStyleAda" />
					<menuitem name="Baan" verb="ActionFormatHighlightStyleBaan" />
					<menuitem name="Lisp" verb="ActionFormatHighlightStyleLisp" />
