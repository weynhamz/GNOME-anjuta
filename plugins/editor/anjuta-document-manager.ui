				<menuitem name="Cut" verb="ActionEditPaste" />
				<menuitem name="Clear" verb="ActionEditClear" />
				<separator name="separator2"/>
				<submenu name="Transform" verb="ActionMenuEditTransform">
					<menuitem name="UpperCase" verb="ActionEditMakeSelectionUppercase" />
					<menuitem name="LowerCase" verb="ActionEditMakeSelectionLowercase" />
