						<menuitem name="HeaderTemplate" verb="ActionEditInsertHeaderTemplate" />
						<menuitem name="ChangeLog" verb="ActionEditInsertChangelog" />
					</submenu>
				</submenu>
			</placeholder>
			<placeholder name="PlaceholderSearchMenus">
				<submenu name="Search" verb="ActionMenuEditSearch">
