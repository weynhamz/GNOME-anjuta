					<menuitem name="FindInFiles" verb="ActionEditSearchInFiles"/>
					<menuitem name="ISearch" verb="ActionEditSearchSelectionISearch"/>
				</submenu>
			</placeholder>
			<submenu name="Goto" verb="ActionMenuEditGoto">
				<placeholder name="PlaceholderGotoMenus">
					<menuitem name="Line" verb="ActionEditGotoLine" />
					<menuitem name="MatchingBrace" verb="ActionEditGotoMatchingBrace" />
