					<menuitem name="BlockStart" verb="ActionEditGotoBlockStart" />
					<menuitem name="BlockEnd" verb="ActionEditGotoBlockEnd" />
					<separator name="separator2"/>
					<menuitem name="PreviousOccurance" verb="ActionEditGotoOccurancePrev" />
					<menuitem name="NextOccurance" verb="ActionEditGotoOccuranceNext" />
