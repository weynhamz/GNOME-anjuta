<!--*- xml -*-->
<Root>
	<menu name="MenuMain">
		<submenu name="MenuFile" verb="ActionMenuFile">
			<placeholder name="PlaceholderFileMenus">
				<menuitem name="New" verb="ActionFileNew" />
				<menuitem name="Open" verb="ActionFileOpen" />
				<menuitem name="Save" verb="ActionFileSave" />
				<menuitem name="SaveAs" verb="ActionFileSaveAs" />
				<menuitem name="SaveAll" verb="ActionFileSaveAll" />
				<menuitem name="Close" verb="ActionFileClose" />
				<menuitem name="CloseAll" verb="ActionFileCloseAll" />
				<menuitem name="Reload" verb="ActionFileReload" />
				<separator name="separator1"/>
			</placeholder>
			<placeholder name="PlaceholderPrintMenus">
				<menuitem name="Print" verb="ActionPrintFile" />
				<menuitem name="PrintPreview" verb="ActionPrintPreview" />
				<separator name="separator3"/>
			</placeholder>
			<placeholder name="PlaceholderRecentMenus">
				<submenu name="RecentFiles" verb="ActionMenuFileRecentFiles" />
			</placeholder>
		</submenu>
