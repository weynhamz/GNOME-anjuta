dnl  **********************************************************
dnl  check if we have svn libraries to build subversion plugin
dnl  (stolen from kdevelop ;-)
dnl  **********************************************************
X
AC_MSG_CHECKING(for Subversion svn-config)
AC_ARG_WITH(subversion-dir,
X		AS_HELP_STRING([--with-subversion-dir=DIR],[Where Subversion is installed ]),
X		[
X				SVNCONFIG="$withval/bin/svn-config"
X		])
X
if test -z "$SVNCONFIG"; then
X		_SVNCONFIG="`svn-config --prefix 2> /dev/null`"
X		if test -n "$_SVNCONFIG"; then
X				SVNCONFIG="$_SVNCONFIG/bin/svn-config"
X		fi
fi
X
AC_SUBST(SVNCONFIG)
if test -x "$SVNCONFIG"; then
X		SVNLD="`$SVNCONFIG --ldflags 2> /dev/null`"
X		SVN_LIB="`$SVNCONFIG --libs --cflags 2> /dev/null` -lsvn_client-1 -lsvn_subr-1"
X		dnl ugly hack for subversion svn-config problems in 0.14.x, to be removed when svn-config is fixed
X		SVN_INCLUDE="`$SVNCONFIG --includes 2> /dev/null` -I$_SVNCONFIG/include/subversion-1/"
X		AC_MSG_RESULT(yes)
else
X		AC_MSG_RESULT(not found)
X
X		dnl just a fallback to debian's config so that it works for me :)
X		AC_ARG_WITH(svn-include,
X								[[  --with-svn-include=DIR   Use the given path to the subversion headers.]],
X								[
X								if test "$withval" != "yes" -a "$withval" != ""; then
X										SVN_INCLUDES=$withval
X								fi
X								])
X		if test -z "$SVN_INCLUDES"; then
X			SVN_INCLUDES="/usr/local/include /usr/include"
X		fi
X		AC_MSG_CHECKING([for Subversion headers])
X		SVN_INCLUDE=""
X		for VALUE in $SVN_INCLUDES ; do
X				if test -f $VALUE/subversion-1/svn_types.h ; then
X						SVN_INCLUDE=$VALUE/subversion-1
X						break
X				fi
X				if test -f $VALUE/svn_types.h ; then
X						SVN_INCLUDE=$VALUE
X						break
X				fi
X		done
X		if test $SVN_INCLUDE ; then
X				AC_MSG_RESULT([found])
X		else
X				AC_MSG_RESULT([not found])
X		fi
X		SVN_LIBS="/usr/local/lib64 /usr/local/lib /usr/lib64 /usr/lib"
X		AC_ARG_WITH(svn-lib,
X						[[  --with-svn-lib=DIR  Use the given path to the subversion libraries.]],
X						[
X							if test "$withval" != "yes" -a "$withval" != ""; then
X								SVN_LIBS=$withval
X							fi
X						])
X		AC_MSG_CHECKING([for Subversion libraries])
X		SVN_LIB=""
X		for VALUE in $SVN_LIBS ; do
X				if ls $VALUE/libsvn_client-1.* 1>/dev/null 2>&1; then
X						SVN_LIB=$VALUE
X						break
X				fi
X		done
X		if test $SVN_LIB ; then
X				AC_MSG_RESULT([found])
X		else
X				AC_MSG_RESULT([not found])
X		fi
fi
X
dnl ******************************************************************
dnl Check for extra libs required by subversion.
dnl FIXME: This should actually be done by subversion and not by us.
dnl ******************************************************************
X
AC_ARG_ENABLE(plugin-subversion,
X  AS_HELP_STRING([--disable-plugin-subversion],[Disable subversion support in Anjuta.]),
X  [ if test "$enableval" = "no"; then
X        user_disabled_subversion=1
X    fi ],
X  [ user_disabled_subversion=0 ] )
X
AC_MSG_CHECKING(if subversion support is disabled)
if test "$user_disabled_subversion" = 1; then
X	AC_MSG_RESULT(yes)
X	SVN_INCLUDE=""
X	SVN_LIB=""
else
X	AC_MSG_RESULT(no)
fi
X
if test -n "$SVN_INCLUDE" ; then
X	dnl ------------------------------------
X	dnl APR. Required by subversion (devel)
X	dnl ------------------------------------
X	have_atr=no
X	PKG_CHECK_MODULES(APR,                            \
X		apr-1, have_apr=yes, have_apr=no)
X	if test x"$have_apr" = "xno"; then
X		SVN_INCLUDE=""
X	fi
X
X	dnl -----------------------------------------
X	dnl APR util. Required by subversion (devel)
X	dnl------------------------------------------
X	have_atr_util=no
X	PKG_CHECK_MODULES(APR_UTILS,                            \
X		apr-util-1, have_apr_util=yes, have_apr_util=no)
X	if test x"$have_apr_util" = "xno"; then
X		SVN_INCLUDE=""
X	fi
X
X	dnl -----------------------------------------
X	dnl NEON. Required by subversion (devel)
X	dnl------------------------------------------
X	have_neon=no
X	PKG_CHECK_MODULES(NEON,                            \
X                neon, have_neon=yes, have_neon=no)
X        if test x"$have_neon" = "xno"; then
X		SVN_INCLUDE=""
X	fi
X
X	dnl Make sure our SVN version is high enough
X	dnl Eww, eww, eww, this is dirty
X	AC_MSG_CHECKING([for subversion >= $SUBVERSION_REQUIRED])
X	AC_LANG([C])
X	dnl Temporarily set CFLAGS so that include dirs are found
X	CFLAGS_TMP=$CFLAGS
X	CFLAGS="-I$SVN_INCLUDE $APR_CFLAGS"
X	AC_COMPILE_IFELSE(
X		[AC_LANG_PROGRAM([[#include <svn_version.h>]],
X			[[
X				#if (SVN_VER_MAJOR < $SVN_MAJOR) || (SVN_VER_MINOR < $SVN_MINOR) || (SVN_VER_PATCH < $SVN_PATCH)
X				#	error "Version too low"
X				#endif
X			]])], AC_MSG_RESULT(yes), have_svn_version=no)
X	if test x"$have_svn_version" = "xno"; then
X		AC_MSG_RESULT(no)
X		SVN_INCLUDE=""
X	fi
X	CFLAGS=$CFLAGS_TMP
fi
X
dnl ------------------------------------------
dnl Finally prepare subversion build flags
dnl ------------------------------------------
X
if test -n "$SVN_INCLUDE" ; then
X	SVN_INCLUDE="-I$SVN_INCLUDE"
X	if test x != "x$SVN_LIB" ; then
X	    SVN_LIB="-L$SVN_LIB $APR_LIBS $APR_UTILS_LIBS $NEON_LIBS -lsvn_client-1 -lsvn_subr-1"
X	else
X	    SVN_LIB="$APR_LIBS $APR_UTILS_LIBS $NEON_LIBS -lsvn_client-1 -lsvn_subr-1"
X	fi
X	SVN_CFLAGS="$APR_CFLAGS $APR_UTILS_CFLAGS $NEON_CFLAGS"
fi
X
